{
    "version": 2,
    "builds": [
      {
        "src": "index.html",
        "use": "@vercel/static-build",
        "config": {
          "zeroConfig": true
        }
      },
      {
        "src": "/src/**/*",
        "use": "@vercel/static-build"
      }
    ],
    "routes": [
      {
        "handle": "filesystem"
      },
      {
        "src": "/(.*)",
        "dest": "index.html"
      }
    ]
  }
  